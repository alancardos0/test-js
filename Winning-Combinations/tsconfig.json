{
  "compilerOptions": {
    "target": "es2019",
    "module": "es2015",
    "sourceMap": true,
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": [
      "jest"
    ]
  },
  "exclude": [
    "**/*.test.ts"
  ]
}